	<queryString>
		<![CDATA[SELECT p.nome AS produto, l.nome AS Loja, IFNULL((SELECT SUM(vep.quantidadeVendida) FROM Venda AS v INNER JOIN VendaEntradaProduto vep ON vep.idVenda = v.id INNER JOIN EntradaProduto ep ON ep.id = vep.idEntradaProduto WHERE p.id = vep.idProduto and v.nomeVendedor like '%'  and v.desativado = 0),0) AS vendida FROM Produto AS p INNER JOIN Loja l ON p.loja_id = l.id where l.id like '%'  order by vendida desc]]>
	</queryString>
