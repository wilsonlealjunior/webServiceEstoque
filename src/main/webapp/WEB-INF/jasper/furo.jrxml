	<queryString>
		<![CDATA[SELECT a.data,a.idusuario as funcionario, l.nome AS loja, aep.quantidade, aep.precoDeVenda,p.nome, aep.quantidade*aep.precoDeVenda as valor FROM Furo a INNER JOIN loja l ON l.id = a.idLoja INNER JOIN itemfuro aep ON aep.idFuro = a.id INNER JOIN entradaproduto ep ON aep.idEntradaProduto = ep.id INNER JOIN produto p ON ep.produto_id = p.id order by funcionario,a.data desc;]]>
